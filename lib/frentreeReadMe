    Future<void> postProcessApprovedExpense(User user, WorkApproval model) async {
      String _docId;

    }int _index = model.docIds.length;

           for (int i = 0; i < _index; i++) {
             _docId = model.docIds[i];
             await firestore
                 .collection(COMPANY)
                 .doc(user.companyCode)
                 .collection(USER)
                 .doc(model.userMail)
                 .collection(EXPENSE)
                 .doc(_docId)
                 .update({
               "isApproved": true,
               "status": "결",
             });
           }


